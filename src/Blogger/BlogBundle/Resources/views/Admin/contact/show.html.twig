
{# src /Blogger/BlogBundle/Resources/views/Admin/contact/index.html.twig #}
{% extends 'BloggerBlogBundle:Admin:base.html.twig' %}


{% block contentHeader %}
     Artciole <small>Lista articole</small>
{% endblock %}
{% block body %}
<div class="row">
        <section class="col-lg-12">
            <div class="nav-tabs-custom">
                
                <div class="tab-content">
                    <div class="tab-pane active">
						<div class="row">
                            <div class="col-md-12">
                                <dl class="dl-horizontal">
                                    <dt>Nume</dt>
                                    <dd>{{ contact.name }}</dd>
                                </dl>
                            </div>
                        </div>
						<div class="row">
                            <div class="col-md-4">
                                <dl class="dl-horizontal">
                                    <dt>Mail</dt>
                                    <dd>{{ contact.email }}</dd>
                                </dl>
                            </div>
							<div class="col-md-4">
                                <dl class="dl-horizontal">
                                    <dt>Continut</dt>
                                    <dd>{{ contact.subject }}</dd>
                                </dl>
                            </div>
						    <div class="col-md-4">
                                <dl class="dl-horizontal">
                                    <dt>Data</dt>
                                    <dd>{{ contact.created|date('Y-m-d h:m') }}</dd>
                                </dl>
                            </div>
                        </div>
						<div class="row">
                            <div class="col-md-12">
                                <dl class="dl-horizontal">
                                    <dt>Subiect</dt>
                                    <dd>{{ contact.body }}</dd>
                                </dl>
                            </div>
                        </div>
                        <hr>
                        <div class="row">
							<div class="col-md-4">
                                <dl class="dl-horizontal">
                                    <dt></dt>
									<dd></dd>
                                </dl>
                            </div>
                        </div>
                        <hr>
                        <div class="clearfix">
						    <div class="pull-right">
                              
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

{% endblock %}